	<queryString>
		<![CDATA[SELECT  em.employee_name, em.employee_id, em.employee_newicno, jp.jobposition_name, dp.department_name, concat(pr.period_month,"/",pr.period_year)as timelinedate , ol.overtimeline_totalhour, ol.overtimeline_date, ol.overtimeline_starttime, ol.overtimeline_endtime, ol.line_desc , ot.verified_date, em2.employee_name as verified_by, ot.overtime_date, dayname(ol.overtimeline_date)
FROM sim_simedu_overtime ot
INNER JOIN sim_simedu_employee em ON em.employee_id = ot.employee_id
INNER JOIN sim_simedu_employee em2 ON em2.employee_id=ot.verified_by 
INNER JOIN sim_simedu_jobposition jp ON jp.jobposition_id = em.jobposition_id
INNER JOIN sim_simedu_department dp ON dp.department_id = em.department_id
INNER JOIN sim_period pr ON pr.period_id = ot.period_id
LEFT JOIN sim_simedu_overtimeline ol ON ol.overtime_id = ot.overtime_id
where ot.overtime_id=$P{ot_id}]]>
	</queryString>
